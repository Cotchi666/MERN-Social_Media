ctor((state) => state);\n  const dispatch = useDispatch();\n  const [user, setUser] = useState(\"\");\n  console.log(\"auth\", auth);\n  useEffect(() => {\n    dispatch(refreshToken());\n  }, [dispatch]);\n  return (\n    <Router>\n      <Alert />\n      <input type=\"checkbox\" id=\"theme\" />\n      <div className=\"App\">\n        <div className=\"main\">\n          {/* put params */}\n          {auth.token && <Header />}\n          {status && <StatusModal />}\n\n          <Route exact path=\"/\" component={auth.token ? Home : Login} />\n          <Route exact path=\"/register\" component={Register} />\n\n          <PrivateRouter exact path=\"/:page\" component={PageRender} />\n          <PrivateRouter exact path=\"/:page/:id\" component={PageRender} />\n        </div>\n      </div>\n    </Router>\n  );\n}\n\nexport default App;\n","/Users/chien/Documents/MERN/frontend/src/redux/store.js",[],"/Users/chien/Documents/MERN/frontend/src/customRouter/PageRender.js",[],"/Users/chien/Documents/MERN/frontend/src/customRouter/PrivateRouter.js",[],"/Users/chien/Documents/MERN/frontend/src/components/StatusModal.js",[],"/Users/chien/Documents/MERN/frontend/src/components/alert/Alert.js",[],"/Users/chien/Documents/MERN/frontend/src/components/header/Header.js",[],"/Users/chien/Documents/MERN/frontend/src/redux/actions/authAction.js",[],"/Users/chien/Documents/MERN/frontend/src/pages/home.js",[],"/Users/chien/Documents/MERN/frontend/src/pages/login.js",["185","186"],"/Users/chien/Documents/MERN/frontend/src/pages/register.js",[],"/Users/chien/Documents/MERN/frontend/src/redux/reducers/index.js",[],"/Users/chien/Documents/MERN/frontend/src/pages/message.js",[],"/Users/chien/Documents/MERN/frontend/src/pages/notify.js",[],"/Users/chien/Documents/MERN/frontend/src/pages/discover.js",[],"/Users/chien/Documents/MERN/frontend/src/pages/profile/[id].js",[],"/Users/chien/Documents/MERN/frontend/src/components/NotFound.js",[],"/Users/chien/Documents/MERN/frontend/src/components/alert/Loading.js",[],"/Users/chien/Documents/MERN/frontend/src/components/header/Menu.js",[],"/Users/chien/Documents/MERN/frontend/src/redux/actions/globalTypes.js",[],"/Users/chien/Documents/MERN/frontend/src/components/alert/Toast.js",[],"/Users/chien/Documents/MERN/frontend/src/components/header/Search.js",["187","188"],"import React, { useEffect, useState } from 'react'\nimport { useSelector, useDispatch } from 'react-redux'\nimport { getDataAPI } from '../../utils/fetchData'\nimport { GLOBALTYPES } from '../../redux/actions/globalTypes'\nimport UserCard from '../UserCard'\nimport LoadIcon from '../../images/Infinity-1s-64px.gif'\n\nconst Search = () => {\n    const [search, setSearch] = useState('')\n    const [users, setUsers] = useState([])\n    const {auth} = useSelector(state => state)\n    const dispatch = useDispatch()\n    const [load, setLoad] = useState(false)\n\n\n    const handleClose = () => {\n        setSearch('')\n        setUsers([])\n    }\n\n    useEffect(async () => {\n      if (!search) return;\n  \n      try {\n        setLoad(true);\n        const res = await getDataAPI(`search?username=${search}`, auth.token);\n        //mongo data\n        setUsers(res.data.users);\n        setLoad(false);\n      } catch (err) {\n        dispatch({\n          type: GLOBALTYPES.ALERT,\n          payload: { error: err.response.data.msg },\n        });\n      }\n    }, [search]);\n    return (\n    <form className=\"search-form\">\n        <input type=\"text\" name = \"search\" value = {search} id = \"search\" title='Enter to search'\n        onChange={e => setSearch(e.target.value.toLowerCase().replace(/ /g,''))}/>\n\n        <div className='search-icon' style={{opacity: search ? 0 : 0.3}}>\n            <span className='material-icons'>search</span>\n            <span>Enter to search</span>\n        </div>\n\n        <div className='close-search' onClick={handleClose} \n        style={{opacity:users.length === 0 ? 0 : 1}}>\n            &times;\n        </div> \n\n        <button type='submit' style={{display: 'none'}}>Search</button>\n\n        {load && <img className='loading' src={LoadIcon} alt='loading'/>}\n\n        <div className='users'>\n            {\n                search && users.map(user =>(\n                        <UserCard\n                        key={user._id}\n                        user={user} \n                        border = \"border\"\n                        handleClose={handleClose}\n                         />\n                ))\n            }\n        </div>\n    </form>\n\n  )\n}\n\nexport default Search\n","/Users/chien/Documents/MERN/frontend/src/redux/reducers/themeReducer.js",[],"/Users/chien/Documents/MERN/frontend/src/redux/reducers/alertReducer.js",[],"/Users/chien/Documents/MERN/frontend/src/redux/reducers/profileReducer.js",[],"/Users/chien/Documents/MERN/frontend/src/redux/reducers/authReducer.js",[],"/Users/chien/Documents/MERN/frontend/src/redux/reducers/statusReducer.js",[],"/Users/chien/Documents/MERN/frontend/src/utils/fetchData.js",[],"/Users/chien/Documents/MERN/frontend/src/utils/valid.js",["189","190","191","192"],"const valid = ({ fullname, username, email, password, cf_password }) => {\n  const err = {};\n\n  if (!fullname) {\n    err.fullname = \"Please add your full name.\";\n  } else if (fullname.length > 25) {\n    err.fullname = \"Full name is up to 25 characters long.\";\n  }\n\n  if (!username) {\n    err.username = \"Please add your user name.\";\n  } else if (username.replace(/ /g, \"\").length > 25) {\n    err.username = \"User name is up to 25 characters long.\";\n  }\n\n  if (!email) {\n    err.email = \"Please add your email.\";\n  } else if (!validateEmail(email)) {\n    err.email = \"Email format is incorrect.\";\n  }\n\n  if (!password) {\n    err.password = \"Please add your password.\";\n  } else if (password.length < 6) {\n    err.password = \"Password must be at least 6 characters.\";\n  }\n\n  if (password !== cf_password) {\n    err.cf_password = \"Confirm password did not match.\";\n  }\n\n  return {\n    errMsg: err,\n    errLength: Object.keys(err).length,\n  };\n};\n\nfunction validateEmail(email) {\n  // eslint-disable-next-line\n  const re =\n    /^(([^<>()[\\]\\\\.,;:\\s@\\\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\\\"]+)*)|(\\\".+\\\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\n  return re.test(email);\n}\n\nexport default valid;\n","/Users/chien/Documents/MERN/frontend/src/components/home/Posts.js",[],"/Users/chien/Documents/MERN/frontend/src/components/home/Status.js",[],"/Users/chien/Documents/MERN/frontend/src/components/profile/Posts.js",[],"/Users/chien/Documents/MERN/frontend/src/components/profile/Info.js",["193"],"import React, {useState, useEffect} from 'react'\nimport {useParams} from 'react-router-dom'\nimport {useSelector, useDispatch} from 'react-redux'\nimport Avatar from '../Avatar'\nimport {getProfileUsers} from '../../redux/actions/profileAction'\nimport EditProfile from './EditProfile'\nimport FollowBtn from '../FollowBtn'\nimport Followers from './Followers'\nimport Followings from './Followings'\n\nconst Info = () => {\n    const { id } = useParams()\n    const { auth, profile } = useSelector(state => state)\n    const dispatch = useDispatch()\n\n    const [userData, setUserData] = useState([])\n    const [onEdit, setOnEdit] = useState(false)\n\n    const [showFollowers, setShowFollowers] = useState(false)\n    const [showFollowings, setShowFollowings] = useState(false)\n\n\n    useEffect(() => {\n        if(id === auth.user._id){\n            setUserData([auth.user])\n        } else{\n            dispatch(getProfileUsers({users: profile.users, id, auth}))\n            const newData = profile.users.filter(user => user._id === id)\n            setUserData(newData)\n        }\n    }, [id, auth, dispatch, profile.users])\n\n    return (\n    <div className='info'>\n        {\n            userData.map(user =>(\n                <div className='info-container' key={user._id}>\n                    <Avatar src={user.avatar} size='supper-avatar'/>\n                    <div className='info-content'>\n                        <div className='info-content-title'>\n                            <h2>{user.username}</h2>\n                            {\n                                user._id === auth.user._id\n                                ? <button className='btn btn-outline-info'\n                                onClick={() => setOnEdit(true)}>\n                                    Edit Profile\n                                </button>\n                                : <FollowBtn user={user}/>\n                            }\n                            \n                        </div>\n\n                        <div className='follow-btn'>\n                            <span className='mr-4' onClick={() => setShowFollowers(true)}>\n                                {user.followers.length} Followers\n                            </span>\n\n                            <span className='ml-4' onClick={() => setShowFollowings(true)}>\n                                {user.followings.length} Followings\n                            </span>\n                        </div>\n\n                        <h6>{user.fullname} <span className='text-danger'>{user.mobile}</span> </h6>\n                        <p className='m-0'>{user.address}</p>\n                        <h6 className='m-0'>{user.email}</h6>\n                        <a href={user.website} target=\"_blank\">\n                            {user.website}\n                        </a>\n                        <p>{user.story}</p>\n                    </div>\n                    {\n                            onEdit && <EditProfile setOnEdit={setOnEdit} />\n                        }\n\n                        {\n                            showFollowers &&\n                            <Followers \n                            users={user.followers} \n                            setShowFollowers={setShowFollowers} \n                            />\n                        }\n                        {\n                            showFollowings &&\n                            <Followings \n                            users={user.followings} \n                            setShowFollowings={setShowFollowings} \n                            />\n                        }\n                </div>\n            )) \n        }\n        \n    </div>\n  )\n}\n\nexport default Info","/Users/chien/Documents/MERN/frontend/src/components/Avatar.js",["194"],"import React from \"react\";\nimport {useSelector, useDispatch  } from 'react-redux'\n\nconst Avatar = ({src, size}) => {\n  const {theme} =useSelector((state) => state);\n  return (\n    <img\n      src={src}\n      alt=\"avatar\"\n      className={size}\n      style={{ filter: `${theme ? \"invert(1)\" : \"invert(0)\"}` }}\n    ></img>\n  );\n};\n\nexport default Avatar;\n","/Users/chien/Documents/MERN/frontend/src/components/UserCard.js",[],"/Users/chien/Documents/MERN/frontend/src/redux/actions/profileAction.js",[],"/Users/chien/Documents/MERN/frontend/src/components/profile/Followers.js",[],"/Users/chien/Documents/MERN/frontend/src/components/profile/EditProfile.js",["195"],"import { handleInput } from \"concurrently/src/defaults\";\nimport React, { useState, useEffect } from \"react\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport {checkImage} from '../../utils/imageUpload'\nimport {GLOBALTYPES} from '../../redux/actions/globalTypes'\nimport {updateProfileUser} from '../../redux/actions/profileAction'\n\n\nconst EditProfile = ({ setOnEdit }) => {\n  const initialState = {\n    fullname: \"\",\n    mobile: \"\",\n    address: \"\",\n    website: \"\",\n    story: \"\",\n    gender: \"\",\n  };\n\n  const [userData, setUserData] = useState(initialState);\n  const { fullname, mobile, address, website, story, gender } = userData;\n  const [avatar, setAvatar] = useState(\"\");\n\n  const { auth, theme } = useSelector((state) => state);\n  const dispatch = useDispatch();\n\n  useEffect(() => {\n    setUserData(auth.user)\n\n  }, [auth.user])\n\n\n  const changeAvatar = (e) => {\n    const file = e.target.files[0]\n\n    const err = checkImage(file)\n    if(err) return dispatch({\n        type: GLOBALTYPES.ALERT, \n        payload: {error:err}\n    })\n    setAvatar(file)\n  };\n\n  const handleInput = (e) => {\n    const { name, value } = e.target;\n    setUserData({ ...userData, [name]: value });\n  };\n\n  const handleSubmit = e => {\n    e.preventDefault();\n    dispatch(updateProfileUser({userData, avatar, auth}))\n  }\n\n  return (\n    <div className=\"edit-profile\">\n      <button\n        className=\"btn btn-danger btn-close\"\n        onClick={() => setOnEdit(false)}\n      >\n        Close\n      </button>\n\n      <form onSubmit={handleSubmit}>\n        <div className=\"info-avatar\">\n          <img\n            src={avatar ? URL.createObjectURL(avatar) : auth.user.avatar}\n            alt=\"avatar\"\n            style={{ filter: theme ? \"invert(1)\" : \"invert(0)\" }}\n          />\n          <span>\n            <i className=\"fas fa-camera\" />\n            <p>Change</p>\n            <input\n              type=\"file\"\n              name=\"file\"\n              id=\"file-up\"\n              accept=\"image/*\"\n              onChange={changeAvatar}\n            />\n          </span>\n        </div>\n\n        <div className=\"form-group\">\n          <label htmlFor=\"fullname\">Fullname</label>\n          <div className=\"position-relative\">\n            <input\n              type=\"text\"\n              className=\"form-control\"\n              id=\"fullname\"\n              name=\"fullname\"\n              value={fullname}\n              onChange={handleInput}\n            />\n            <small\n              className=\"text-danger position-absolute\"\n              style={{\n                top: \"50%\",\n                right: \"5px\",\n                transform: \"translateY(-50%)\",\n              }}\n            >\n              {fullname.length}/25\n            </small>\n          </div>\n        </div>\n\n        <div className=\"form-group\">\n          <label htmlFor=\"mobile\">Mobile</label>\n          <input\n            type=\"text\"\n            name=\"mobile\"\n            value={mobile}\n            className=\"form-control\"\n            onChange={handleInput}\n          />\n        </div>\n\n        <div className=\"form-group\">\n          <label htmlFor=\"address\">Address</label>\n          <input\n            type=\"text\"\n            name=\"address\"\n            value={address}\n            className=\"form-control\"\n            onChange={handleInput}\n          />\n        </div>\n\n        <div className=\"form-group\">\n          <label htmlFor=\"website\">Website</label>\n          <input\n            type=\"text\"\n            name=\"website\"\n            value={website}\n            className=\"form-control\"\n            onChange={handleInput}\n          />\n        </div>\n\n        <div className=\"form-group\">\n          <label htmlFor=\"story\">Story</label>\n          <textarea\n            name=\"story\"\n            value={story}\n            className=\"form-control\"\n            onChange={handleInput}\n          />\n          <small className=\"text-danger d-block text-right\">\n            {story.length}/25\n          </small>\n        </div>\n\n        <label htmlFor=\"gender\">Gender</label>\n        <div className=\"input-group-prepend px-0 mb-4\">\n          <select\n            name=\"gender\"\n            id=\"gender\"\n            value={gender}\n            className=\"custom-select text-capitalize\"\n            onChange={handleInput}\n          >\n            <option value=\"male\">Male</option>\n            <option value=\"female\">Female</option>\n            <option value=\"other\">Other</option>\n          </select>\n        </div>\n\n        <button className=\"btn btn-info w-100\" type=\"submit\">\n          Save\n        </button>\n      </form>\n    </div>\n  );\n};\n\nexport default EditProfile;\n","/Users/chien/Documents/MERN/frontend/src/components/FollowBtn.js",[],"/Users/chien/Documents/MERN/frontend/src/components/profile/Followings.js",[],"/Users/chien/Documents/MERN/frontend/src/utils/imageUpload.js",[],{"ruleId":"196","replacedBy":"197"},{"ruleId":"198","replacedBy":"199"},{"ruleId":"200","severity":1,"message":"201","line":18,"column":10,"nodeType":"202","messageId":"203","endLine":18,"endColumn":14},{"ruleId":"200","severity":1,"message":"204","line":18,"column":16,"nodeType":"202","messageId":"203","endLine":18,"endColumn":23},{"ruleId":"200","severity":1,"message":"205","line":7,"column":10,"nodeType":"202","messageId":"203","endLine":7,"endColumn":29},{"ruleId":"200","severity":1,"message":"201","line":16,"column":10,"nodeType":"202","messageId":"203","endLine":16,"endColumn":14},{"ruleId":"206","severity":1,"message":"207","line":21,"column":15,"nodeType":"208","endLine":36,"endColumn":6},{"ruleId":"206","severity":1,"message":"209","line":36,"column":8,"nodeType":"210","endLine":36,"endColumn":16,"suggestions":"211"},{"ruleId":"212","severity":1,"message":"213","line":41,"column":27,"nodeType":"214","messageId":"215","endLine":41,"endColumn":28,"suggestions":"216"},{"ruleId":"212","severity":1,"message":"213","line":41,"column":52,"nodeType":"214","messageId":"215","endLine":41,"endColumn":53,"suggestions":"217"},{"ruleId":"212","severity":1,"message":"213","line":41,"column":61,"nodeType":"214","messageId":"215","endLine":41,"endColumn":62,"suggestions":"218"},{"ruleId":"212","severity":1,"message":"213","line":41,"column":65,"nodeType":"214","messageId":"215","endLine":41,"endColumn":66,"suggestions":"219"},{"ruleId":"220","severity":1,"message":"221","line":66,"column":25,"nodeType":"222","messageId":"223","endLine":66,"endColumn":64,"fix":"224"},{"ruleId":"200",